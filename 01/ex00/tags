!_TAG_FILE_FORMAT	2	/extended format; --format=1 will not append ;" to lines/
!_TAG_FILE_SORTED	1	/0=unsorted, 1=sorted, 2=foldcase/
!_TAG_PROGRAM_AUTHOR	Darren Hiebert	/dhiebert@users.sourceforge.net/
!_TAG_PROGRAM_NAME	Exuberant Ctags	//
!_TAG_PROGRAM_URL	http://ctags.sourceforge.net	/official site/
!_TAG_PROGRAM_VERSION	5.8	//
CC	Makefile	/^CC		=	clang++ -std=c++98$/;"	m
CFLAGS	Makefile	/^CFLAGS	=	-Wall -Wextra -Werror$/;"	m
LFLAGS	Makefile	/^LFLAGS	=	-lpthread$/;"	m
NAME	Makefile	/^NAME	=	Zombie$/;"	m
OBJS	Makefile	/^OBJS	=	$(SRCS:.cpp=.o)$/;"	m
RM	Makefile	/^RM		=	rm -f$/;"	m
SPLASHART	Makefile	/^define SPLASHART$/;"	m
SRCS	Makefile	/^SRCS	=	.\/main.cpp .\/newZombie.cpp .\/randomChump.cpp .\/Zombie.cpp$/;"	m
Zombie	Zombie.cpp	/^Zombie::Zombie(str name)$/;"	f	class:Zombie
Zombie	Zombie.cpp	/^Zombie::Zombie(void)$/;"	f	class:Zombie
Zombie	Zombie.hpp	/^class Zombie$/;"	c
announce	Zombie.cpp	/^void	Zombie::announce(void)$/;"	f	class:Zombie
main	main.cpp	/^int	main(void)$/;"	f
name	Zombie.hpp	/^		str name;$/;"	m	class:Zombie
newZombie	newZombie.cpp	/^Zombie* newZombie(str name)$/;"	f
randomChump	randomChump.cpp	/^void	randomChump(str name)$/;"	f
str	Zombie.cpp	/^typedef std::string str;$/;"	t	file:
str	Zombie.hpp	/^typedef std::string str;$/;"	t
str	newZombie.cpp	/^typedef std::string str;$/;"	t	file:
~Zombie	Zombie.cpp	/^Zombie::~Zombie(void)$/;"	f	class:Zombie
